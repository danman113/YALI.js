// Class Example
class Doughnut {
  init(name) {
    this.name = name;
  }

  cook() {
    var name = "";
    if (this.name) {
      name = this.name + " ";
    }
    print "You fry the " + name + "Doughnut until golden brown.";
  }
}

class BostonCream < Doughnut {
  init() {
    super.init("Boston Cream");
  }
}

Doughnut("Cruller").cook();

BostonCream().cook();

fun hello () {
  print "Would you like some breakfast?";
}

print BostonCream;
print Doughnut;
print BostonCream();
print hello;